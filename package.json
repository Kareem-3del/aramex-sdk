{
  "name": "@kareem-3del/aramex-sdk",
  "version": "1.1.0",
  "description": "Complete Aramex API SDK for Node.js - Shipping, Tracking, Rate Calculator, and Location Services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./nestjs": {
      "types": "./dist/nestjs/index.d.ts",
      "default": "./dist/nestjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run copy-wsdl",
    "copy-wsdl": "node -e \"require('fs').cpSync('src/wsdl', 'dist/wsdl', {recursive: true})\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kareem-3del/aramex-sdk.git",
    "directory": "aramex-sdk"
  },
  "bugs": {
    "url": "https://github.com/Kareem-3del/aramex-sdk/issues"
  },
  "homepage": "https://github.com/Kareem-3del/aramex-sdk/tree/master/aramex-sdk#readme",
  "keywords": [
    "aramex",
    "shipping",
    "tracking",
    "logistics",
    "nestjs",
    "typescript"
  ],
  "author": {
    "name": "kareem-3del",
    "url": "https://kareem-3del.com",
    "github": "https://github.com/kareem-3del",
    "linkedin": "https://linkedin.com/in/kareem-3del"
  },
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    }
  },
  "dependencies": {
    "soap": "^1.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.21",
    "jest": "^29.5.0",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0"
  }
}
